{
    "version": 2,
    "builds": [
        {
            "src": "vite.config.js",
            "use": "@vercel/static-build",
            "config": {
                "distDir": "dist",
                "devCommand": "vite",
                "buildCommand": "vite build",
                "outputDirectory": "build"
            }
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/index.html"
        }
    ]
}
